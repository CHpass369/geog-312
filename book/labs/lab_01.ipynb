      "sAN fRANCISCO\n",
      "Los Francisco\n"
     ]
    }
   ],
   "source": [
    "city = \"San Francisco\"\n",
    "\n",
    "city_lowercase = city.lower()\n",
    "city_uppercase = city.upper()\n",
    "city_capitalize = city.capitalize()\n",
    "city_title = city.title()\n",
    "city_swapcase = city.swapcase()\n",
